@namespace("no.nav.dagpenger.events.avro")
protocol Dagpenger {
  enum JournalpostType {
      NY, GJENOPPTAK, ETTERSENDING, MANUELL, UKJENT
  }

  enum BrukerType {
      PERSON, ORGANISASJON
  }

  record Søker {
    string identifikator;
  }

  record Dokument {
    string dokumentId;
    string navSkjemaId;
  }

  record Journalpost {
    string id;
    union { null, JournalpostType } journalpostType = null;
    union { null, array<Dokument> } dokumentListe = null;
    union { null, Søker } søker = null;
    union { null, string } behandleneEnhet = null;
    union { null, string } fagsakId = null;
  }

  record Behov {
    string _id;
    union { null, Journalpost } journalpost = null;
  }
}
